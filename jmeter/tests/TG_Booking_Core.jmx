      <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="script">if (!props.get(&quot;urlBase&quot;)) {
    // Local development environment
    props.put(&quot;urlBase&quot;, &quot;localhost&quot;)
    props.put(&quot;port&quot;, &quot;8080&quot;)
    log.info(&quot;Set base url for development: ${props.get(&quot;urlBase&quot;)}:${props.get(&quot;port&quot;)}&quot;)
} else if (props.get(&quot;urlBase&quot;) == &quot;localhost&quot;) {
    // Already dev mode
    log.info(&quot;Dev mode detected, keeping port 8080&quot;)
    props.put(&quot;port&quot;, &quot;8080&quot;)
} else {
	def urlBase = props.get(&quot;urlBase&quot;)
	if (urlBase.startsWith(&quot;http://&quot;)) {
	    urlBase = urlBase.replaceFirst(&quot;http://&quot;, &quot;&quot;)
	} else if (urlBase.startsWith(&quot;https://&quot;)) {
	    urlBase = urlBase.replaceFirst(&quot;https://&quot;, &quot;&quot;)
	}
	urlBase = urlBase.replaceFirst(&quot;/&quot;, &quot;&quot;)
	props.put(&quot;urlBase&quot;, urlBase)
    // Deployment environment
    log.info(&quot;Deployment mode detected, base URL: ${props.get(&quot;urlBase&quot;)}&quot;)
    props.put(&quot;port&quot;, &quot;&quot;)
}

log.info(&quot;üìò CSV Data Loaded:&quot;)
log.info(&quot;  role      = &quot; + vars.get(&quot;role&quot;))
log.info(&quot;  email     = &quot; + vars.get(&quot;email&quot;))
log.info(&quot;  password  = &quot; + vars.get(&quot;password&quot;))

</stringProp>
        <stringProp name="scriptLanguage">groovy</stringProp>
      </JSR223PreProcessor>
